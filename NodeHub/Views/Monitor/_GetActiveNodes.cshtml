@model IEnumerable<NodeHub.Models.Monitor.NodeModel>
@using System.Numerics;
<h4 class="w-100">Active list</h4>
<ul class="list-group">
    @foreach (var node in Model)
    {
        <li id="active" class="list-group-item border-secondary text-center" style="border-left-width: 5px !important;">@node.ID</li>
    }
</ul>
